{
    "user": "BROWSERSTACK_USERNAME",
    "key": "BROWSERSTACK_ACCESS_KEY",
    "capabilities": {
        "browserstack.debug": true
    },
    "environments": [
        {
            "browser": "chrome",
            "os": "Windows",
            "os_version": "10",
            "chromeOptions": {
                "args": [
                    "--start-maximized",
                    "--disable-geolocation"
                ]
            }
        }
    ]
}